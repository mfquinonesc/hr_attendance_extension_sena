<!-- her the date has to be 5 hours ahead  -->

<odoo>
    <data>

        <record id="ir_cron_auto_checkout_attendances" model="ir.cron">
            <field name="name">Auto Checkout Attendances</field>
            <field name="model_id" ref="model_hr_attendance"/>
            <field name="state">code</field>
            <field name="code">model.auto_checkout_attendances()</field>
            <field name="interval_type">days</field>
            <field name="interval_number">1</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="nextcall" eval="(DateTime.now().replace(hour=0, minute=0, second=0, microsecond=0) + relativedelta(days=0)).strftime('%Y-%m-%d %H:%M:%S')"/>

        </record>

    </data>
</odoo>